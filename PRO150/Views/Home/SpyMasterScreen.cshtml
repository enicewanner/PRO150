                        if (Card.redColorCount == 9)
                        {
                            Card.redComplete = true;
                        }
                        break;
                }
            }
            else if (Card.isAssassin && !Card.redComplete && Card.blueComplete && Card.whiteComplete)
            {
                Card.color25 = "Red";


                <div class="@Card.color24">
                    @Card.word24
                </div>

                Card.redColorCount++;
                if (Card.redColorCount == 9)
                {
                    Card.redComplete = true;
                }

            }
            else if (Card.isAssassin && Card.redComplete && !Card.blueComplete && Card.whiteComplete)
            {
                Card.color25 = "Blue";


                <div class="@Card.color24">
                    @Card.word24
                </div>
                Card.blueColorCount++;
                if (Card.blueColorCount == 8)
                {
                    Card.blueComplete = true;
                }

            }
            else if (Card.isAssassin && Card.redComplete && Card.blueComplete && !Card.whiteComplete)
            {
                Card.color25 = "White";


                <div class="@Card.color24">
                    @Card.word24
                </div>
                Card.whiteColorCount++;
                if (Card.whiteColorCount == 7)
                {
                    Card.whiteComplete = true;
                }

            }
            else if (!Card.isAssassin && Card.redComplete && Card.blueComplete && Card.whiteComplete)
            {
                Card.color25 = "Black";


                <div class="@Card.color24">
                    @Card.word24
                </div>


                if (Card.color25.Contains("Black"))
                {
                    Card.isAssassin = true;
                }
            }
            else if (Card.isAssassin && Card.redComplete && Card.blueComplete && Card.whiteComplete)
